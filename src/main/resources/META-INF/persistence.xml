<?xml version="1.0" encoding="UTF-8"?>
<persistence version="3.0" xmlns="https://jakarta.ee/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://jakarta.ee/xml/ns/persistence https://jakarta.ee/xml/ns/persistence/persistence_3_0.xsd">
  <!-- Define Persistence Unit -->
  <persistence-unit name="org.tarang_persistece_unit" transaction-type="JTA">
    <jta-data-source>jdbc/tarangdb</jta-data-source>
    <class>org.bhaduri.tarang.entities.Calltable</class>
    <class>org.bhaduri.tarang.entities.Users</class>
    <class>org.bhaduri.tarang.entities.Validatecall</class>
    <class>org.bhaduri.tarang.entities.Scrips</class>
  </persistence-unit>
</persistence>
